{
    "manifest_version": 2,
    "name": "Alter",
    "version": "1.0",

    "description": "Redirect to privacy-friendly alternatives",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "storage"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "Alter",
        "default_popup": "popup.html",
        "default_icon": {
            "128": "./logo.png"
        }
    }
}
